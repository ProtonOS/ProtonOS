<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Library</OutputType>
    <TargetFramework>net10.0</TargetFramework>
    <AssemblyName>ProtonOS.Drivers.Ahci</AssemblyName>
    <RootNamespace>ProtonOS.Drivers.Storage.Ahci</RootNamespace>
    <Nullable>enable</Nullable>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <ImplicitUsings>disable</ImplicitUsings>
    <!-- Suppress CS0436: Our types intentionally shadow .NET BCL types -->
    <NoWarn>$(NoWarn);CS0436</NoWarn>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="../../../../ddk/DDK.csproj" />
    <ProjectReference Include="../fat/Fat.csproj" />
  </ItemGroup>

</Project>
